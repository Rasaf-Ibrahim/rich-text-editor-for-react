{
    "name": "rich-text-editor-for-react",
    "version": "0.7.1",
    "description": "A Rich Text Editor for React",
    "author": "Rasaf Ibrahim",
    "license": "MIT",
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c -w",
        "login": "npm adduser",
        "publish": "npm publish"
    },
    "dependencies": {
        "rich-text-editor-for-react-dependencies": "^1.2.2"
    },
    "devDependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.11.16",
        "@mui/material": "^5.11.3",
        "@rollup/plugin-commonjs": "^25.0.1",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.1",
        "immer": "^10.0.2",
        "postcss": "^8.4.24",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup": "^3.25.1",
        "rollup-plugin-dts": "^5.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "tslib": "^2.5.3",
        "typescript": "^5.1.3",
        "use-immer": "^0.9.0"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./hook": "./dist/hook.js",
        "./display-output": "./dist/display-output.js"
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/types/index.d.ts"
            ],
            "hook": [
                "./dist/types/hook.d.ts"
            ],
            "display-output": [
                "./dist/types/display-output.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "react",
        "quill",
        "rich-text-editor",
        "wysiwyg",
        "rte"
    ]
}
